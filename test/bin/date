#! /bin/sh

# Order;
# critical
# to
# now

case $@ in

	# first_critical_zpool
	'-j -f %Y-%m-%d.%H:%M:%S 2017-06-16.10:25:47 +%s'|'+%s --date=2017-06-16 10:25:47')
		echo 1497601547
		exit 0
		;;

	# last_warning_zpool
	'-j -f %Y-%m-%d.%H:%M:%S 2017-06-16.10:25:48 +%s'|'+%s --date=2017-06-16 10:25:48')
		echo 1497601548
		exit 0
		;;

	# first_warning_zpool
	'-j -f %Y-%m-%d.%H:%M:%S 2017-07-17.10:25:47 +%s'|'+%s --date=2017-07-17 10:25:47')
		echo 1500279947
		exit 0
		;;

	# last_ok_zpool
	'-j -f %Y-%m-%d.%H:%M:%S 2017-07-17.10:25:48 +%s'|'+%s --date=2017-07-17 10:25:48')
		echo 1500279948
		exit 0
		;;

	# first_ok_zpool
	'-j -f %Y-%m-%d.%H:%M:%S 2017-08-17.10:25:48 +%s'|'+%s --date=2017-08-17 10:25:48')
		echo 1502958348
		exit 0
		;;

	# now
	'+%s')
		echo 1502958348
		exit 0
		;;

	# first_critical_zpool
	'+%Y-%m-%dT%H:%M:%SZ --date @1497601547')
		echo 2017-06-16T10:25:47Z
		exit 0
		;;

	# first_warning_zpool
	'+%Y-%m-%dT%H:%M:%SZ --date @1500279947')
		echo 2017-07-17T10:25:47Z
		exit 0
		;;

	# first_ok_zpool
	'+%Y-%m-%dT%H:%M:%SZ --date @1502958348')
		echo 2017-08-17T10:25:48Z
		exit 0
		;;

esac
