#! /bin/sh

case $@ in

	# ok_zpool
	'+%s --date=2017-07-17 10:25:48')
		echo 1500279948
		exit 0
		;;

	# warning_zpool
	'+%s --date=2017-06-16 10:25:48')
		echo 1497601548
		exit 0
		;;

	# critical_zpool
	'+%s --date=2017-06-16 10:25:47')
		echo 1497601547
		exit 0
		;;

	# now
	'+%s')
		echo 1502958348
		exit 0
		;;

esac
