#! /bin/sh

case $@ in
	'+%s date=2017-07-09 00:24:08')
		echo 1499552648
		exit 0
		;;

	# ok_dataset
	'-d @1502914537 +%Y-%m-%dT%H:%M:%SZ')
		echo '2017-08-16T22:15:37Z'
		exit 0
		;;

	# warning_dataset
	'-d @1502834464 +%Y-%m-%dT%H:%M:%SZ')
		echo '2017-08-16T00:01:04Z'
		exit 0
		;;

	# critical_dataset
	'-d @1469273510 +%Y-%m-%dT%H:%M:%SZ')
		echo '2016-07-23T13:31:50Z'
		exit 0
		;;

esac


2017-07-09.00:24:08
